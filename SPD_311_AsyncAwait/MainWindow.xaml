<Window x:Class="SPD_311_AsyncAwait.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SPD_311_AsyncAwait"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <ToolBar DockPanel.Dock="Top">
            <Button Click="Button_Click">Generate</Button>
        </ToolBar>
        <ProgressBar Height="20" DockPanel.Dock="Bottom" IsIndeterminate="True"></ProgressBar>
        <ListBox Name="list"></ListBox>

    </DockPanel>
</Window>
